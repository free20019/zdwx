	url: basePath + "login/index",
	data:{},
	timeout : 180000,
	dataType: 'json',
	success:function(data){
		power_buts = data.power_but;
		if(power_buts.indexOf('维修审核审核')<0)
			$("#shMaintain-Aud").hide();
		if(power_buts.indexOf('维修审核交费')<0)
			$("#shMaintain-pay").hide();
	},
	error: function(XMLHttpRequest, textStatus, errorThrown) {
	}         
