					layer.msg('必须选中一行');
				}
			});
			$('#shMaintain-pay').on('click', function () {
				if ($(queryCheckBox + ':checked').length > 0) {
					var postdata = [];
					layer.confirm('你确定要将选中的数据设置为"交费"吗？', {
						btn: ['确定', '取消'],
						offset: '100px'
					}, function (layerIndex) {
						// 遍历要选中的数据
						$(queryCheckBox + ':checked').parents('tr').each(function (index, item) {
							postdata.push($(item).data('database'));
							console.info('each:', $(item).data('database'), postdata);
						});
						var ids = "";
						for (var int = 0; int < postdata.length; int++) {
							ids += postdata[int].RR_ID+",";
						}
						$.ajax({
							type: "POST",
							url: basePath + "wx/paywxmx",
							data:'postData={"id":"'+ids+'"}',
							timeout : 180000,
							dataType: 'json',
							success:function(data){
								if(data.info=='操作成功'){
									layer.close(layerIndex);
									layer.msg('操作成功');
									findwxmx();
								}else{
									layer.close(layerIndex);
									layer.msg('操作失败');
								}
							},
							error: function(){
							}
						});
					}, function (layerIndex) {
						layer.close(layerIndex);
					});
				} else {
					layer.msg('至少选中一行');
				}
			});
			$('#shMaintain-Dc').on('click', function(){
				var postData = {
						"STIME":$("#wxHappen-stateTime").val(),
						"ETIME":$("#wxHappen-endTime").val(),
						"QK":$("#wxHappen-block").val(),
						"GS":$("#wxHappen-company").val(),
						"CPHM":$("#wxHappen-licensePlate").val(),
						"RY":$("#wxHappen-maintenanceStaff").val(),
						"WXLX":$("#wxHappen-maintenanceType").val(),
						"ZDLX":$("#wxHappen-terminalType").val()
					};
					url = basePath + "wx/findwxmxexcle?postData="
						+ encodeURI(encodeURI(JSON.stringify(postData))), window.open(url)
			})
		});
	})(jQuery);
	function fundcomp2(obj,id){
		$.ajax({
			 type: "POST",
			url: basePath + "common/findcompowner",
			data:{
				"owner_name":$(id).val()
			},
			timeout : 180000,
			dataType: 'json',
			success:function(data){
				$("#wxHappen-company").val('')
// 				for(var i=0; i<data.datas.length;i++){
// 					$(obj).append('<li data-value="'+data.datas[i].id+'">'+data.datas[i].name+'</li>');
// 				}
				var data2 = data.datas;
				$(obj).find('.iFList').html("");
				for (var i = 0; i < data2.length; i++) {
					var cphms="<li data-value='"+data2[i].id+"'>"+data2[i].name+"</li>";
					$(obj).find('.iFList').append(cphms);
				}
				$(obj).find('.iFList').on('click', function () {
					if (event.stopPropagation){event.stopPropagation();}else if (window.event) {window.event.cancelBubble = true;}
				}).find('li').off('click').on('click', function () {
					$(this).addClass('selected').siblings('.selected').removeClass('selected');
					$(obj).find('input').data('value', $(this).data('value')).val($(this).text()).end().removeClass('selected');
				});
			},
			error: function(){
			}         
		});
	}
</script>
