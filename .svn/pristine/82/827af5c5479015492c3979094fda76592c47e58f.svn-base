			$("#wxServer-accessoriescosts").val(data ? data.PARTS_TOTAL_PRICE : '');
			$("#wxServer-workexpense").val(data ? data.LABOR_COST : '');
			$("#wxServer-paymentmethod").val(data ? data.PAY_METHOD : '');
			$("#wxServer-billingmethod").val(data ? data.BILLING_METHOD : '');
			$("#wxServer-invoicenumber").val(data ? data.INVOICE_NUMBER : '');

			$('#wxServer-symptoms').val(data ? data.RM_MALFUNCTION : '');
			$('#wxServer-maintenanceLocation').val(data ? data.RA_ADDR : '');
			$('#wxServer-maintenanceContent').val(data ? data.RC_CONTENT : '');
		}

		$(function () {
			$('#wxHappen-stateTime').datetimepicker(datetimeDefaultOption);
			$('#wxHappen-endTime').datetimepicker(datetimeDefaultOption);
			/*保存方法*/
			$('#shMaintain-save').on('click', function () {
				var item = $(queryCheckBox+':checked').parents('tr').data('database');
				$.ajax({
						 type: "POST",
						url: basePath + "wx/wxsh",
						data:'postData={"type":"'+$('#shMaintain-maintenanceState').val()+
						'","id":"'+item.RR_ID+'","note":"'+$('#shMaintain-reason').val()+
						'"}',
						timeout : 180000,
						dataType: 'json',
						success:function(data){
							if(data.info=='审核成功'){
								$('#shMaintain-modalMaintenanceReview').modal('hide');
								layer.msg('审核成功');
								findwxmx();
							}else{
								layer.msg('审核失败');
							}
						},
						error: function(){
						}         
					});
				/*关闭弹出框*/
			});

			var shMaintainTabel = new CheckBox('#shMaintain-tabel');
			$('#shMaintain-Aud').on('click', function () {
				if ($(queryCheckBox + ':checked').length === 1) {
